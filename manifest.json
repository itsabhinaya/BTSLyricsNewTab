{
  "name": "BTS Random Lyrics",
  "short_name": "BTS Lyrics",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "See a random BTS Lyrics when you open a new tab.",
  "author": "Abhinaya Konduru",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "BTS Lyrics"
  }
}